{
  "manifest_version": 2,
  "name": "album-cover-led",
  "version": "0.3.0",

  "description": "Send currently playing album to led matrix",

  "content_scripts": [
    {
      "matches": ["https://*.bandcamp.com/*"],
      "js": ["index.js"]
    }
  ],

  "permissions": [
    "http://192.168.1.26/*"
  ]
}

